package org.opentutorials.javatutorials.conditionaloperator;

public class andDemo {

	public static void main(String[] args) {
		
		if (true && true) {
			System.out.println("true");
		}
		
		if (true && false) {
			System.out.println("false");
		}
		
		if(false && true) {
			System.out.println("false");
			
		}
		
		if(false && false) {
			System.out.println("false");
		}

	}

}
